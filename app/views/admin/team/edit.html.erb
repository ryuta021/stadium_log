<main>
   <h1> チーム編集画面</h1>
   <%= form_for(@team, url: admin_team_path) do |f| %>
        <div class="form-group">
            <label>チーム名</label>
            <%= f.text_field :team, class: "form-control", autocomplete: 'off',placeholder: "入力してください" %>
        </div>
        <%= f.submit "チーム登録" , class: "btn btn-success col-lg-5" %>
    <% end %>
    <%= link_to "削除",  admin_team_path(@team.id), method: :delete ,class: "btn btn-primary col-lg-5" %>
</main>
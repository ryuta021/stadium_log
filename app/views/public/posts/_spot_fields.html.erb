<div class="nested-fields form-inline">
    <div class="form-group">
      <label class="control-label">スポットNo</label>
      <%= f.text_field :spot_number %>
    </div>
    <div class="form-group">
      <label class="control-label">スポット金額</label>
      <%= f.text_field :spot_place %>
    </div>
    <div class="form-group">
      <label class="control-label">スポット滞在時間</label>
      <%= f.text_field :spot_time %>
    </div>
    <div class="form-group">

        <label class="control-label">スポット場所</label>
        <%= f.text_field :address %>
    </div>
    <div class="form-group">
      <label class="control-label">投稿内容</label>
      <%= f.text_field :spot_content %>
    </div>
    <%= link_to_remove_association 'プランを削除', f %>
</div>

